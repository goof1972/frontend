{
  "extends": [
    "tslint:recommended",
    "tslint-react",
    "tslint-config-prettier"
  ],
  "linterOptions": {
    "exclude": [
      "config/**/*.js",
      "node_modules/**/*.ts"
    ]
  },
  "rules": {
    "no-console": false,
    "no-unused-expression": false,
    "prefer-const": false,
    "interface-over-type-literal": false
  }
}